{
	"name": "overlay",
	"version": "1.0.0",
	"description": "",
	"main": "dist/main/index.js",
	"author": {
		"name": "VisualSource"
	},
	"overwolf": {
		"packages": ["overlay"]
	},
	"build": {
		"nodeGypRebuild": false,
		"appId": "us.visualsource.r6random",
		"asar": true,
		"overwolf": {
			"disableAdOptimization": false
		},
		"icon":"icon.png",
		"productName": "R6Random",
		"directories": {
			"output": "build"
		},
		"files": ["dist/**/*", "*.js", "*.html", "*.exe"],
		"copyright": "Copyright 2023 VisualSource",
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": ["x64"]
				}
			],
			"artifactName": "${productName}-Setup-${version}.${ext}",
			"publisherName": "VisualSource",
			"legalTrademarks": "Copyright 2023 VisualSource"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		}
	},
	"scripts": {
		"preview": "electron-vite preview",
		"dev": "electron-vite dev",
		"prebuild": "electron-vite build",
		"start": "ow-electron .",
		"build:ow-electron": "ow-electron-builder --c.extraMetadata.name=R6Random --publish=never"
	},
	"keywords": [],
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@overwolf/ow-electron-builder": "^24.13.8",
		"electron": "32.0.1",
		"electron-vite": "^2.3.0",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"@overwolf/ow-electron": "^28.3.5",
		"@overwolf/ow-electron-packages-types": "^0.0.12"
	}
}
